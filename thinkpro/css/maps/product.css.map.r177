r\n    background-size: cover;\r\n}\r\n\r\n%flex-wrap {\r\n    display: -webkit-flex;\r\n    display: -ms-flexbox;\r\n    display: flex;\r\n    -webkit-flex-wrap: wrap;\r\n    -ms-flex-wrap: wrap;\r\n    flex-wrap: wrap;\r\n}\r\n\r\n.flex {\r\n    @extend %flex;\r\n}\r\n\r\n.flex-wrap {\r\n    @extend %flex-wrap;\r\n}\r\n\r\n.thumb_img {\r\n    display: block;\r\n    height: 0;\r\n    position: relative;\r\n    overflow: hidden;\r\n\r\n    img {\r\n        position: absolute;\r\n        left: 0;\r\n        top: 0;\r\n        width: 100%;\r\n        height: 100%;\r\n        object-fit: cover;\r\n        transition: all .4s ease 0s;\r\n    }\r\n}\r\n\r\n.thumb_5x3 {\r\n    padding-bottom: 60%;\r\n}\r\n\r\n.thumb_5x4 {\r\n    padding-bottom: 80%;\r\n}\r\n\r\n.thumb_5x5 {\r\n    padding-bottom: 100%;\r\n}\r\n\r\n.thumb_16x9 {\r\n    padding-bottom: 56.25%;\r\n}","@import \"var.scss\";\r\n\r\n.transition_ease {\r\n    transition: 0.5s ease;\r\n    -webkit-transition: 0.5s ease;\r\n}\r\n\r\n.hotline--fillter {\r\n    .hotline--fillter__btn {\r\n        display: none !important\r\n    }\r\n}\r\n\r\n// ================= Product Detail==================\r\n.page-detail {\r\n    .sticky {\r\n        top: 94px\r\n    }\r\n\r\n    .swiper-slide {\r\n        .thumb_img {\r\n            padding-bottom: 100%;\r\n        }\r\n    }\r\n\r\n    .list-gallery {\r\n        display: flex;\r\n        justify-content: center;\r\n        margin: 0 -5px;\r\n\r\n        .items {\r\n            width: 117px;\r\n            margin: 0 5px;\r\n        }\r\n\r\n        .thumb_img {\r\n            box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.129);\r\n            background: #CACACA;\r\n            border-radius: 3px;\r\n            padding-bottom: 76.07%;\r\n            transition: 0.3s ease;\r\n\r\n            &:hover {\r\n                box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.3);\r\n            }\r\n        }\r\n    }\r\n\r\n    .title_mobile {\r\n        display: none\r\n    }\r\n\r\n    .title-product {\r\n        font-size: 32px;\r\n        font-weight: bold;\r\n        line-height: 1.3;\r\n        color: var(--black);\r\n        display: inline-block;\r\n    }\r\n\r\n    .code {\r\n        display: inline-block;\r\n        font-size: 14px;\r\n        font-weight: 600;\r\n        text-transform: uppercase;\r\n    }\r\n\r\n    .view-more {\r\n        margin-top: 5px;\r\n        display: inline-block;\r\n        color: var(--dark_gray);\r\n        font-weight: 500;\r\n\r\n        svg {\r\n            width: 14px;\r\n            height: 14px;\r\n        }\r\n    }\r\n\r\n    hr {\r\n        border-top: 1px solid var(--gray100);\r\n        margin: 24px 0 16px 0;\r\n    }\r\n\r\n    .lable-title {\r\n        font-weight: 600;\r\n        font-size: 20px;\r\n        margin-bottom: 8px;\r\n    }\r\n\r\n    .border.radius {\r\n        border: 2px solid var(--gray100);\r\n        padding: 16px;\r\n        border-radius: 5px;\r\n        margin-bottom: 8px;\r\n        @extend .transition_ease;\r\n\r\n        hr {\r\n            margin: 20px 0;\r\n        }\r\n\r\n        &:hover {\r\n            border: 2px solid var(--color_main);\r\n        }\r\n    }\r\n\r\n    .price {\r\n        color: #D4002B;\r\n\r\n        .iconfont-tag {\r\n            margin-right: 4px;\r\n        }\r\n\r\n        &.tag {\r\n            text-align: right;\r\n        }\r\n    }\r\n}\r\n\r\n.productdetail {\r\n    .border-bottom {\r\n        margin-bottom: 32px;\r\n        padding-bottom: 20px;\r\n        border-bottom: 1px solid var(--gray100);\r\n    }\r\n\r\n    &__thumb {\r\n        width: 534px;\r\n    }\r\n\r\n    &__info {\r\n        width: 690px;\r\n        width: calc(100% - 534px);\r\n        font-size: 14px;\r\n        position: relative;\r\n        padding-left: 90px;\r\n        line-height: 1.5;\r\n    }\r\n\r\n    .flex_2 {\r\n        justify-content: space-between;\r\n\r\n        .description {\r\n            width: 287px;\r\n        }\r\n\r\n        .service {\r\n            wid